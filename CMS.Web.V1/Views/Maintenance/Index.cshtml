@using CMS.Web.V1.Models.Helper
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="@HttpContext.Current.Session["CurrentLanguageCode"]">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title | Web</title>

    @{Html.RenderAction("_SeoSetting", "Partial");}
    <!-- Favicon -->
    <link rel="icon" href="/Content/Admin/img/logo.jpg" />
    <!--Bootstrap-->
    <link rel="stylesheet" type="text/css" href="/Content/css/bootstrap.css">
    <!--Font-Awesome-->
    <link rel="stylesheet" type="text/css" href="/Content/css/font-awesome.css">
    <!--Angular JS-->
    <!--<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.7.9/angular.min.js"></script>-->
    <script src="~/Scripts/angularjs/angular.min.js" type="text/javascript"></script>
    <script src="~/Scripts/Angularjs/angular-sanitize.min.js" type="text/javascript"></script>

    <!-- Fonts-->
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet">
    <style>
        body, p, span, h1, h2, h3, h4, h5, h6, .h1, .h2, .h3, .h4, .h5, .h6 {
            font-family: 'Montserrat', sans-serif;
        }
    </style>
    <!--Stylesheets-->
    <link rel="stylesheet" type="text/css" href="/Content/css/style.css?@HttpContext.Current.Application["CSSVer"]">
    <link rel="stylesheet/less" type="text/css" href="/Content/css/themecolor.less">

    <!--[if lt IE 9]>
        <script src="js/html5shiv.js"></script>
        <script src="js/respond.min.js"></script>
      [endif]-->

</head>
<body data-spy="scroll" data-target=".navbar-default" data-offset="100" class="bg-warning" ng-app="ngApp">

    <section id="maintenance" ng-controller="maintenancecontroller">
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <img src="~/Files/Upload/company_images/logo.png" alt="" class="img-responsive logo">
                </div>
                <div class="col-sm-8 col-xs-offset-2">
                    <div class="row animebox">
                        <div class="col-xs-3"> <img src="~/Content/images/maintenance_left.gif" class="img-responsive maintenanceleft" /></div>
                        <div class="col-xs-9"> <img src="~/Content/images/maintenance_right.gif" class="img-responsive" /></div>
                    </div>
                </div>
                <div class="c-both"></div>
                <div class="col-sm-12">
                    <h2 class="maintenance-title">@(("Maintenance.Title").toLanguage())</h2>
                </div>
                <div class="col-sm-2 col-sm-offset-3"><span class="infoItem"><i class="fa fa-envelope"></i> {{company.Mail}}</span></div>
                <div class="col-sm-2"><span class="infoItem"><i class="fa fa-phone"></i> {{company.Phone}}</span></div>
                <div class="col-sm-2"><span class="infoItem"><i class="fa fa-fax"></i> {{company.Fax}}</span></div>

            </div>
        </div>
    </section>

    <!--Jquery-->
    <script type="text/javascript" src="~/Scripts/js/jquery.min.js"></script>
    <!--Boostrap-Jquery-->
    <script type="text/javascript" src="~/Scripts/js/bootstrap.js"></script>
    <!--SweetAlert-->
    <script src="~/Content/Admin/vendors/sweetalert/sweetalert.min.js"></script>

    <!--Angular-->
    <script src="~/Scripts/angularApp.js?@HttpContext.Current.Application["JSVer"]" type="text/javascript"></script>
    <script src="~/Scripts/Pages/maintenance.js?@HttpContext.Current.Application["JSVer"]" type="text/javascript"></script>


</body>
</html>




