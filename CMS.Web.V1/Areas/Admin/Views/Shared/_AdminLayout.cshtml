@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width initial-scale=1.0">
    <title>@ViewBag.Title | CMS</title>
    <meta name="robots" content="noindex,nofollow" />
    <link rel="icon" href="/Content/Admin/img/logo.jpg" />
    <!-- GLOBAL MAINLY STYLES-->
    <link href="~/Content/Admin/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Admin/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/Admin/vendors/themify-icons/css/themify-icons.css" rel="stylesheet" />
    <link href="~/Content/Admin/vendors/iziToast/css/iziToast.css" rel="stylesheet" />
    <!-- THEME STYLES-->
    <link href="~/Content/Admin/css/main.min.css?@HttpContext.Current.Application["CSSVer"]" rel="stylesheet" />
    <!--ANGULARJS-->
    <script src="~/Scripts/angularjs/angular.min.js" type="text/javascript"></script>
    <!-- PAGE LEVEL STYLES-->
    @RenderSection("PageLevelStyles", required: false)
</head>

<body class="fixed-navbar" ng-app="ngPanelApp">
    <div class="page-wrapper">
            @{Html.RenderAction("TopMenu", "Partial");}
            @{Html.RenderAction("LeftMenu", "Partial");}
        <div class="content-wrapper">
            @RenderBody()

            <footer class="page-footer">
                <div class="font-13">@DateTime.Now.Year © <b>Web CMS v1</b> - All rights reserved.</div>
                <a class="px-4" href="#" target="_blank">HE</a>
                <div class="to-top"><i class="fa fa-angle-double-up"></i></div>
            </footer>
        </div>
    </div>

    <div class="sidenav-backdrop backdrop"></div>
    <div class="preloader-backdrop">
        <div class="page-preloader">Yükleniyor</div>
    </div>

    <script src="~/Content/Admin/vendors/jquery/dist/jquery.min.js" type="text/javascript"></script>
    <script src="~/Content/Admin/vendors/popper.js/dist/umd/popper.min.js" type="text/javascript"></script>
    <script src="~/Content/Admin/vendors/bootstrap/dist/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Content/Admin/vendors/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="~/Content/Admin/vendors/metisMenu/dist/metisMenu.js" type="text/javascript"></script>
    <script src="~/Content/Admin/vendors/iziToast/js/iziToast.js" type="text/javascript"></script>
    <script src="~/Content/Admin/vendors/sweetalert/sweetalert.min.js"></script>
    <!-- CORE SCRIPTS-->
    <script src="~/Content/Admin/js/app.min.js?@HttpContext.Current.Application["JSVer"]" type="text/javascript"></script>
    <script src="~/Scripts/angularApp.js?@HttpContext.Current.Application["JSVer"]" type="text/javascript"></script>
    <!-- PAGE LEVEL SCRIPTS-->
    @RenderSection("PageLevelScripts", required: false)
</body>

</html>
